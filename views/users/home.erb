<h1>"Welcome, <%= @user.name %>!"</h1>
<h2>Dashboard</h2>
<h3>Bank Balance</h3>
<% @account_codes.each do |account| %>
  <a href="/user/account/<%= account[:name] %>">
    <h3><%= account[:name] %>: (<%= account[:amount] %>)</h3>
  </a>
<% end %>

<a href="/sessions/logout">Log Out</a>
<a href="/users/settings">Settings</a>
<a href="/bankstatement">Reconcile Bank Statement</a>